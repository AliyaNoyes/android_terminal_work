<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.terminal_work.android.terminal_work">

    <application
        android:allowBackup="true"
        android:icon="@drawable/start_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/start_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoadActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddProjectActivity" />
        <activity android:name=".DailyBillActivity" />
        <activity android:name=".SumBillActivity"></activity>
        <activity android:name=".StatisticsActivity" />
        <activity android:name=".RegisterActivity"></activity>
    </application>

</manifest>